        <activity
            android:name=".Olvido"
            android:label="@string/title_activity_olvido" >
        </activity>
        <activity
            android:name=".Fin"
            android:label="@string/title_activity_fin" >
        </activity>
        <activity
            android:name=".ComentariosActivity"
            android:label="@string/title_activity_comentarios" >
        </activity>
    </application>

</manifest>
